{% set logo = config.theme.logo %}
<a href="{{ home_url }}" class="logo">
{% if logo %}
    {% set logo_image = page.find('/home').media[config.theme.logo] %}
    {% set logo = logo_image.cropResize( 200, 100 ).url %}
    <img src="{{ logo }}" alt="{{ site.title }}" />
{% else %}
    {% include('@images/candle.svg') %}
{% endif %}
</a>